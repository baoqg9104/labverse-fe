  "changelog": {
    "title": "Changelog",
    "lead": "Below you'll find a record of all significant changes to Labverse.",
    "versionPrefix": "Version",
    "labels": {
      "New": "New",
      "Improved": "Improved",
      "Fixed": "Fixed",
      "Removed": "Removed",
      "Deprecated": "Deprecated",
      "Upcoming": "Upcoming"
    },
    "entries": {
      "v1_4_0": {
        "title": "Release: Richer Learning Experience",
        "details": "Major quality-of-life updates, new content features, and several bug fixes.",
        "changes": {
          "0": "Introduced the Changelog page to track releases.",
          "1": "Added advanced quiz analytics on the Result screen.",
          "2": "Faster lab loading and smoother transitions.",
          "3": "Resolved an issue where avatars failed to load for some users."
        }
      },
      "v1_3_2": {
        "title": "Stability & UX fixes",
        "changes": {
          "0": "Payment retry flow now handles network interruptions.",
          "1": "Updated onboarding copy and illustrations.",
          "2": "Added a tooltip on the ranking page describing scoring rules."
        }
      },
      "v1_3_0": {
        "title": "Feature: Team Labs",
        "details": "Collaboration features for teams and instructors.",
        "changes": {
          "0": "Team creation and management for lab sharing.",
          "1": "Search in Forum now supports filters and sorting.",
          "2": "Fixed RSS feed generation for new labs."
        }
      },
      "v1_1_0": {
        "title": "UI Improvements & Font Fixes",
        "details": "Polished visuals and several UX fixes to make the site feel snappier and more consistent.",
        "changes": {
          "0": "Fixed inconsistent font rendering across multiple browsers and platforms.",
          "1": "Refined spacing, typography, and component alignment for clearer hierarchy.",
          "2": "Improved CTAs, reduced visual noise, and made key interactions faster.",
          "3": "Added subtle micro-interactions for better feedback on actions."
        }
      },
      "v1_0_0": {
        "title": "Initial public launch",
        "details": "Labverse public launch: core learning content, community features, and basic gamification.",
        "changes": {
          "0": "Public launch of Labverse with core learning content and hands-on labs.",
          "1": "User accounts, progress tracking, and basic gamification (badges & streaks).",
          "2": "Forum and community features to ask questions and share knowledge."
        }
      },
      "v1_5_0": {
        "title": "Planned: Collaboration & AI",
        "details": "Planned features that will bring collaborative labs and AI-guided assistance.",
        "changes": {
          "0": "AI-powered hints and contextual assistance for quizzes and labs (planned).",
          "1": "In-platform collaborative sessions for Team Labs (planned)."
        }
      }
    }
  },
